    "browser-cookies": "^1.2.0",
    "classnames": "^2.2.6",
    "faye": "^1.2.4",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-bulma-components": "^3.2.0",
    "react-copy-to-clipboard": "^5.0.2",
    "react-crud-table": "0.0.14",
    "react-d3-components": "^0.9.1",
    "react-datepicker": "^2.14.1",
    "react-dom": "^16.12.0",
